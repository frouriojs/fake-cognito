{
  "name": "server",
  "scripts": {
    "predev": "run-s generate migrate:deploy",
    "dev": "run-p dev:*",
    "dev:build": "node ./scripts/build.dev.js",
    "dev:server": "node-dev --enable-source-maps --respawn index.js",
    "dev:ssl": "local-ssl-proxy --key ./certificates/server.key --cert ./certificates/server.crt --source 5052 --target 5050",
    "dev:frourio": "frourio --watch",
    "dev:prisma": "prisma generate --watch",
    "build": "npm run generate && node ./scripts/build.prod.js",
    "lint": "prisma format",
    "generate": "run-s generate:prisma generate:frourio",
    "generate:prisma": "prisma generate",
    "generate:frourio": "frourio",
    "migrate:dev": "prisma migrate dev && npx prisma db seed",
    "migrate:dev:createonly": "prisma migrate dev --create-only",
    "migrate:deploy": "prisma migrate deploy && npx prisma db seed",
    "migrate:reset": "prisma migrate reset",
    "start": "npm run start:client & npm run start:server & npm run start:ssl",
    "start:client": "serve ../client/out -l $CLIENT_PORT",
    "start:server": "prisma migrate deploy && node --enable-source-maps prisma/seed.js && node --enable-source-maps index.js",
    "start:ssl": "local-ssl-proxy --key ./certificates/server.key --cert ./certificates/server.crt --source $SSL_PORT --target $PORT",
    "test": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "prisma": {
    "seed": "node -r esbuild-register prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.645.0",
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/formbody": "^7.4.0",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/static": "^7.0.4",
    "@prisma/client": "^5.19.1",
    "aspida": "^1.14.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "get-jwks": "^9.0.2",
    "jsbn": "^1.1.0",
    "jwt-decode": "^4.0.0",
    "local-ssl-proxy": "^2.0.5",
    "node-jose": "^2.2.0",
    "nodemailer": "^6.9.15",
    "otplib": "^12.0.1",
    "prisma": "^5.19.1",
    "serve": "^14.2.3",
    "ulid": "^2.3.0",
    "velona": "^0.8.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@aspida/axios": "^1.14.0",
    "@types/busboy": "^1.5.4",
    "@types/jsbn": "^1.2.33",
    "@types/node": "^22.5.4",
    "@types/node-jose": "^1.1.13",
    "@types/nodemailer": "^6.4.15",
    "@types/request-promise-native": "^1.0.21",
    "axios": "^1.7.7",
    "esbuild": "^0.23.1",
    "esbuild-node-externals": "^1.14.0",
    "esbuild-register": "^3.6.0",
    "frourio": "^1.1.0",
    "inbucket-js-client": "^1.0.1",
    "node-dev": "^8.0.0",
    "notios": "^0.5.3",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
